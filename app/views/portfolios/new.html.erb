<% provide(:title, 'Add Portfolio') %>

<div class = "row">
  <div class="col-md-6 col-md-offset-3">
    <h1>Add Portfolio</h1>
    <hr>
    <%= form_for @portfolio do |f| %>
      <%= render "shared/error_messages", object: f.object %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Add Portfolio", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>